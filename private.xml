<?xml version="1.0"?>
<root>
    <item>
        <name>Remap Caps Lock to Hyper</name>
        <appendix>OS X doesn't have a Hyper. This maps F3 to Control + Option + Command.</appendix>
        
        <identifier>caps_lock_to_hyper</identifier>
        
        <autogen>
            --KeyToKey--
            KeyCode::F3, 
            
            KeyCode::COMMAND_L,
            ModifierFlag::CONTROL_L | ModifierFlag::OPTION_L
        </autogen>
    </item>
</root>
